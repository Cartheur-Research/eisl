(defun ~eql (x y)
  (let* ((str1 (convert x <string>))
         (str2 (convert y <string>))
         (len (- (min (length str1) (length str2)) 1)))
    (string= (subseq str1 0 len) (subseq str2 0 len))))

(defun nearp (x y)
  (let ((epsiron 1.0e-10))
     (and (< x (+ y epsiron))
          (> x (- y epsiron)))))

